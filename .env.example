## subgraph_network: 
## - mainnet: balancer
## - kovan: balancer-kovan
REACT_APP_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/balancer-labs/{subgraph_network}

## private key without leading '0x'
ETH_PRIVATE_KEY={eth_private_key}

GAS_PRICE=10

