# Configuration file

APPNAME=Hummingbot Gateway API
NODE_ENV=dev
PORT=5000
PROTOCOLS=['celo', 'terra', 'balancer', 'eth']

# Celo

# Terra

TERRA_LCD_URL=https://tequila-lcd.terra.dev
